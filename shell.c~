#include <stdio.h>
#include <stdlib.h>
#include <unistd.h>
#include <string.h>

void parse(char[] in){

  strtok(in,"\n");
  
  char *array[50];
  char *s = in;
  

  int i = 0;
  while(s){
    array[i] = strsep(&s," ");
    //printf("%d: %s\n",i, array[i]);
    i++;
  }

  array[i] = 0;
  execvp(array[0], array);
}

int main() {
  char s1[256];
  fgets(s1,sizeof(s1),stdin);
  
  
  return 0;
}
